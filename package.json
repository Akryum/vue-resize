{
  "name": "vue-resize",
  "version": "0.5.0",
  "description": "Detect DOM element resizing",
  "author": {
    "name": "Guillaume Chau",
    "email": "guillaume.b.chau@gmail.com"
  },
  "scripts": {
    "build": "yarn run build:browser && yarn run build:es && yarn run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "prepublishOnly": "yarn run test",
    "test": "yarn run build && cd ./docs-src && yarn run test"
  },
  "main": "dist/vue-resize.umd.js",
  "module": "dist/vue-resize.esm.js",
  "unpkg": "dist/vue-resize.min.js",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/compiler-sfc": "^3.0.0-beta.20",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "clean-css": "^4.1.8",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-alpha.9",
    "fs-extra": "^9.0.1",
    "rollup": "^2.21.0",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-vue": "^6.0.0-beta.6",
    "vue": "^3.0.0-beta.20",
    "vue-cli-plugin-vue-next": "~0.1.3"
  },
  "peerDependencies": {
    "vue": "^3.0.0-beta.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/Akryum/vue-resize/issues"
  },
  "homepage": "https://github.com/Akryum/vue-resize#readme",
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akryum/vue-resize.git"
  },
  "watch": {
    "build": "src/*.js"
  }
}
